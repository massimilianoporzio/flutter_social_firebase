    "version": "0.2.0",
    "configurations": [
        {
            "name": "flutter_social_firebase",
            "request": "launch",
            "type": "dart",
            "program": "lib/main.dart",
            "args": [
              "--dart-define-from-file",
              "api-keys.json"
            ]
        },
        {
            "name": "flutter_social_firebase (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile",
            "program": "lib/main.dart",
            "args": [
              "--dart-define-from-file",
              "api-keys.json"
            ]
        },
        {
            "name": "flutter_social_firebase (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release",
            "program": "lib/main.dart",
            "args": [
              "--dart-define-from-file",
              "api-keys.json"
            ]
        }
    ]
